! This setting is for chromebook jp keyboard

clear lock
clear control
clear mod4

add control = Caps_Lock Control_L Control_R
add mod4 = Super_L

! For Japanese keyboard
keycode 66 = Control_L Control_L NoSymbol NoSymbol

! For Chromebook
keycode 133 = Control_L Control_L NoSymbol NoSymbol
keycode 105 = Super_L
keycode 37 = Mode_switch

keycode 111 = Up Up Page_Up Page_Up
keycode 113 = Left Left Home Home
keycode 114 = Right Right End End
keycode 116 = Down Down Page_Down Page_Down
keycode 97 = Shift_L Shift_L Shift_L Shift_L
keycode 22 = BackSpace BackSpace Delete Delete

keycode 51 = Return Return Return Return
keycode 132 = backslash bar backslash bar
