! This setting is for thinkpad X240 keyboard

clear lock
clear control
clear mod1

! Caps lock => Ctrl
add control = Caps_Lock Control_L Control_R
keycode 66 = Control_L Control_L NoSymbol NoSymbol

! missing Alt_L => Alt_L
add mod1 = Alt_L
keycode 64 = Alt_L Alt_L Alt_L Alt_L

! Alt_R => Henkan
! Set Henkan to Fcitx & Emacs IME activation key
keycode 108 = Henkan Henkan NoSymbol NoSymbol

! Insert => End
keycode 118 = End End NoSymbol NoSymbol

! Ctrl_R => Alt
!keycode 105 = Alt_R Alt_R NoSymbol NoSymbol

! print screen => Alt_R
!keycode 107 = Alt_R Alt_R NoSymbol NoSymbol
